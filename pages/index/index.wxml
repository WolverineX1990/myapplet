<!--index.wxml-->
<view class="container">
	<canvas
    class="cropper"
    disable-scroll="true"
    bindtouchstart="touchStart"
    bindtouchmove="touchMove"
    bindtouchend="touchEnd"
    canvas-id="cropper">
  </canvas>
  <button bindtap="selectPhoto">选择图片</button>
  <button bindtap="save">保存</button>
</view>
